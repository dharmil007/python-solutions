from bisect import insort,bisect_left,bisect_right
class Solution:
    # @param A : tuple of integers
    # @return an integer
    def lis(self, A):
        
        d = [A[0]]
        n = len(A)
        for i in range(1,n):
            if d[-1] < A[i]:
                d.append(A[i])
            else:
                idx = bisect_left(d,A[i])
                d[idx] = A[i]
            
        return len(d)

s0 = Solution()
s0.lis([0, 8, 4, 12, 2, 10, 6, 14, 1, 9, 5, 13, 3, 11, 7, 15])